{
    "manifest_version": 2,
  
    "name": "Journal Club Plugin",
    "description": "This extension will submit paper suggestion for journal club",
    "version": "1.1",
  
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "icons": { 
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },

    "permissions": [
        "tabs",
        "https://arxiv.org/"
    ]
}