{
    "manifest_version": 2,
  
    "name": "Journal Club Plugin",
    "description": "This extension will add a webpage to a Google doc",
    "version": "1.0",
  
    "browser_action": {
        "default_icon": "icon.png"
    },

    "icons": { 
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "permissions": [
        "tabs",
        "https://arxiv.org/"
    ]
}